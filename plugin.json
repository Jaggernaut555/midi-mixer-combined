{
  "$schema": "./node_modules/midi-mixer-plugin/plugin.schema.json",
  "id": "ca.jaggernaut.midi-mixer.combined",
  "name": "Combined",
  "type": "node",
  "version": "1.1.0",
  "author": "Jaggernaut",
  "main": "lib/index.js",
  "icon": "J.ico",
  "settings": {
    "brightnessLabel": {
      "label": "Brightness Settings",
      "type": "status"
    },
    "brightnessEnabled": {
      "label": "Enable Brightness",
      "type": "toggle",
      "fallback": false
    },
    "brightnessStatus": {
      "label": "Brightness Status",
      "type": "status",
      "fallback": "Not active"
    },
    "voiceMeeterLabel": {
      "label": "Voicemeeter Settings",
      "type": "status"
    },
    "voicemeeterEnabled": {
      "label": "Enable Voicemeeter",
      "type": "toggle",
      "fallback": false
    },
    "notes": {
      "label": "Note",
      "type": "status",
      "fallback": "Reload plugin after changing any of these settings. Check the info tab for explanations of the customization options."
    },
    "maxdb": {
      "label": "Max db",
      "type": "slider",
      "fallback": 12,
      "min": -60,
      "max": 12
    },
    "mindb": {
      "label": "Min db",
      "type": "slider",
      "fallback": -60,
      "min": -60,
      "max": 12
    },
    "customStripAssign": {
      "label": "Custom Strip Assign",
      "type": "text"
    },
    "customStripMute": {
      "label": "Custom Strip Mute",
      "type": "text"
    },
    "customStripRun": {
      "label": "Custom Strip Run",
      "type": "text"
    },
    "enableStripLimits": {
      "label": "Enable Strip Limiter Groups",
      "type": "toggle",
      "fallback": false
    },
    "BusToggles": {
      "label": "Custom Bus Toggle Buttons",
      "type": "text",
      "fallback": "strip0:A1,!B2;strip1:A1"
    },
    "vmstatus": {
      "label": "Voicemeeter Status",
      "type": "status",
      "fallback": "Not connected"
    },
    "runbutton": {
      "label": "run",
      "type": "button"
    },
    "OBSLabel": {
      "label": "OBS Settings",
      "type": "status"
    },
    "OBSEnabled": {
      "label": "Enable OBS",
      "type": "toggle",
      "fallback": false
    },
    "OBSaddress": {
      "label": "OBS WebSocket Address",
      "required": false,
      "type": "text",
      "fallback": "ws://localhost:4455"
    },
    "OBSpassword": {
      "label": "OBS WebSocket Password",
      "required": false,
      "type": "password"
    },
    "OBSstatus": {
      "label": "Status",
      "type": "status",
      "fallback": "Disconnected"
    },
    "OBSreconnect": {
      "label": "Reconnect",
      "type": "button"
    },
    "OBSmeterMultiplier": {
      "label": "Meter Scaling Multiplier",
      "type": "text",
      "fallback": "1"
    }
  }
}