{
  "$schema": "./node_modules/midi-mixer-plugin/plugin.schema.json",
  "id": "ca.jaggernaut.midi-mixer.combined",
  "name": "Combined",
  "type": "node",
  "version": "1.3.2",
  "author": "Jaggernaut",
  "main": "lib/index.js",
  "icon": "J.ico",
  "settings": {
    "brightnessLabel": {
      "label": "Brightness Settings",
      "type": "status"
    },
    "brightnessEnabled": {
      "label": "Enable Brightness",
      "type": "toggle",
      "fallback": false
    },
    "brightnessStatus": {
      "label": "Brightness Status",
      "type": "status",
      "fallback": "Not active"
    },
    "voiceMeeterLabel": {
      "label": "Voicemeeter Settings",
      "type": "status"
    },
    "voicemeeterEnabled": {
      "label": "Enable Voicemeeter",
      "type": "toggle",
      "fallback": false
    },
    "notes": {
      "label": "Note",
      "type": "status",
      "fallback": "Reload plugin after changing any of these settings. Check the info tab for explanations of the customization options."
    },
    "maxdb": {
      "label": "Max db",
      "type": "slider",
      "fallback": 12,
      "min": -60,
      "max": 12
    },
    "mindb": {
      "label": "Min db",
      "type": "slider",
      "fallback": -60,
      "min": -60,
      "max": 12
    },
    "customStripAssign": {
      "label": "Custom Strip Assign",
      "type": "text"
    },
    "customStripMute": {
      "label": "Custom Strip Mute",
      "type": "text"
    },
    "customStripRun": {
      "label": "Custom Strip Run",
      "type": "text"
    },
    "enableStripLimits": {
      "label": "Enable Strip Limiter Groups",
      "type": "toggle",
      "fallback": false
    },
    "BusToggles": {
      "label": "Custom Bus Toggle Buttons",
      "type": "text",
      "fallback": "strip0:A1,!B2;strip1:A1"
    },
    "vmstatus": {
      "label": "Voicemeeter Status",
      "type": "status",
      "fallback": "Not connected"
    },
    "runbutton": {
      "label": "run",
      "type": "button"
    },
    "OBSLabel": {
      "label": "OBS Settings",
      "type": "status"
    },
    "OBSEnabled": {
      "label": "Enable OBS",
      "type": "toggle",
      "fallback": false
    },
    "OBSaddress": {
      "label": "OBS WebSocket Address",
      "required": false,
      "type": "text",
      "fallback": "ws://localhost:4455"
    },
    "OBSpassword": {
      "label": "OBS WebSocket Password",
      "required": false,
      "type": "password"
    },
    "OBSstatus": {
      "label": "Status",
      "type": "status",
      "fallback": "Disconnected"
    },
    "OBSreconnect": {
      "label": "Reconnect",
      "type": "button"
    },
    "OBSmeterMultiplier": {
      "label": "Meter Scaling Multiplier",
      "type": "text",
      "fallback": "1"
    },
    "WaveLinkLabel": {
      "label": "Wave Link Settings",
      "type": "status"
    },
    "WaveLinkEnabled": {
      "label": "Enable Wave Link",
      "type": "toggle",
      "fallback": false
    },
    "connectionStatus": {
      "label": "Connection status",
      "type": "status",
      "fallback": "Disconnected"
    },
    "outputDevicesCount": {
      "label": "Output Devices",
      "type": "status"
    },
    "outputDeviceSelected": {
      "label": "Current Selected Output Device",
      "type": "status"
    },
    "outputDevice0": {
      "label": "Output Device 1",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice0Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice1": {
      "label": "Output Device 2",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice1Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice2": {
      "label": "Output Device 3",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice2Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice3": {
      "label": "Output Device 4",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice3Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice4": {
      "label": "Output Device 5",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice4Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice5": {
      "label": "Output Device 6",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice5Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice6": {
      "label": "Output Device 7",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice6Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice7": {
      "label": "Output Device 8",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice7Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice8": {
      "label": "Output Device 9",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice8Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "outputDevice9": {
      "label": "Output Device 10",
      "type": "status",
      "fallback": "Disabled"
    },
    "outputDevice9Enabled": {
      "label": "Enable button and add to switch rotation",
      "type": "toggle"
    },
    "PushToTalkLabel": {
      "label": "PushToTalk Settings",
      "type": "status"
    },
    "PushToTalkEnabled": {
      "label": "Enable PushToTalk",
      "type": "toggle",
      "fallback": false
    },
    "keycode": {
      "label": "Keycode list. Re-activate plugin to update",
      "type": "text",
      "fallback": "17(ctrl)"
    },
    "wheelkeycodes": {
      "label": "Wheel keycode list. Re-activate plugin to update",
      "type": "text",
      "fallback": "~37(left)[~40(down)]/~39(right)[~38(up)]"
    },
    "XinputEnable": {
      "label": "Enable Xinput controller",
      "type": "toggle",
      "fallback": false
    },
    "mouseInputEnable": {
      "label": "Enable mouse control",
      "type": "toggle",
      "fallback": false
    },
    "mouseInputRelative": {
      "label": "Mouse Movement based on turn speed",
      "type": "toggle",
      "fallback": false
    },
    "SpotifyLabel": {
      "label": "Spotify Settings",
      "type": "status"
    },
    "SpotifyEnabled": {
      "label": "Enable Spotify",
      "type": "toggle",
      "fallback": false
    },
    "SpotifyClientID": {
      "label": "Client ID",
      "required": true,
      "type": "text",
      "fallback": "None"
    },
    "SpotifyClientSecret": {
      "label": "Client Secret",
      "required": true,
      "type": "password",
      "fallback": "None"
    },
    "SpotifyAccessToken": {
      "label": "Access Token",
      "type": "password",
      "fallback": "None"
    },
    "SpotifyRefreshToken": {
      "label": "Refresh Token",
      "type": "password",
      "fallback": "None"
    },
    "SpotifyExpressStatus": {
      "label": "Express server status",
      "type": "status",
      "fallback": "Not Running"
    },
    "SpotifyRunLogin": {
      "label": "Run Login Server",
      "type": "button"
    },
    "HueLabel": {
      "label": "Hue Settings",
      "type": "status"
    },
    "HueEnabled": {
      "label": "Enable Hue",
      "type": "toggle",
      "fallback": false
    },
    "hueip": {
      "label": "Hue Bridge IP / Address",
      "required": true,
      "type": "text"
    },
    "hueuser": {
      "label": "Hue Bridge User",
      "required": true,
      "type": "password"
    },
    "synctime": {
      "label": "Sync interval (seconds)",
      "required": false,
      "type": "text",
      "fallback": "30"
    }
  }
}